.jumbotron
  .container
    %h1 All videos

    %table(class='demo')
      %thead
        %th{width: "560px"}= ""
        %th= "Title"
        %th= "Views"
        %th= "Likes"
        %th= "Dislikes"
        %th= "Rating"

      - @videos.each do |video|
        %tr
          %td
            %iframe{:allowfullscreen => "", :frameborder => "0", :height => "315", :src => "https://www.youtube.com/embed/#{video.uid}", :width => "560"}
          %td= video.title
          %td= video.views
          %td= video.likes
          %td= video.dislikes
          %td= number_to_percentage(video.rating)
